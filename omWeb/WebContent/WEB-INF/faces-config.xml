<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<converter>
		<converter-id>selectItemConverter</converter-id>
		<converter-class>inet.module.mdm.jsf.converter.DefaultSelectItemConverter</converter-class>
	</converter>
	<converter>
		<converter-id>identifiedObjectTypeConverter</converter-id>
		<converter-for-class>inet.module.mdm.parameters.IdentifiedObjectType</converter-for-class>
		<converter-class>inet.module.mdm.jsf.converter.IdentifiedObjectTypeConverter</converter-class>
	</converter>
	<converter>
		<description>trims a String (deletes leading and trailling blanks)</description>
		<display-name>TrimStringConverter</display-name>
		<converter-for-class>java.lang.String</converter-for-class>
		<converter-class>inet.module.mdm.jsf.converter.TrimStringConverter</converter-class>
	</converter>
	<validator>
		<validator-id>inet.module.mdm.jsf.validators.DatabaseValidator</validator-id>
		<validator-class>inet.module.mdm.jsf.validators.DatabaseValidator</validator-class>
		<property>
			<property-name>tableName</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<property-name>columnName</property-name>
			<property-class>java.lang.String</property-class>
		</property>
	</validator>
	<validator>
		<validator-id>inet.module.mdm.jsf.validators.ZipCodeValidator</validator-id>
		<validator-class>inet.module.mdm.jsf.validators.ZipCodeValidator</validator-class>
		<property>
			<property-name>country</property-name>
			<property-class>java.lang.String</property-class>
		</property>
	</validator>
	<validator>
		<description></description>
		<display-name>sumValidator</display-name>
		<validator-id>sumValidator</validator-id>
		<validator-class>at.icnc.om.validators.InvoiceValidator</validator-class>
	</validator>
	
	
	<managed-bean>
        <managed-bean-name>documentManager</managed-bean-name>
        <managed-bean-class>
            org.icepdf.examples.jsf.viewer.view.DocumentManager
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <!-- set default demo file to load,  file must be present in demo folder -->
        <managed-property>
            <property-name>documentDemoFile</property-name>
            <value>ICEpdf Developers Guide.pdf</value>
        </managed-property>
        <!-- demo version has configurable font engine -->
        <managed-property>
            <property-name>demo</property-name>
            <value>true</value>
        </managed-property>
    </managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>searchStructureProvider</managed-bean-name>
		<managed-bean-class>inet.module.mdm.jsf.beans.SearchStructureProvider</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Provides Properties from the mdm.properties</description>
		<managed-bean-name>properties</managed-bean-name>
		<managed-bean-class>inet.module.mdm.jsf.beans.PropertiesBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sitesBean</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.SitesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataExporter</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.DataExporter</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>invoiceLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.InvoiceBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>incometypeLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.IncometypeBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.UserBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>invoicestateLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.InvoicestateBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>intervalLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.IntervalBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.CustomerBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>concernLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.ConcernBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>contactpersonLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.ContactpersonBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerstateLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.CustomerstateBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>costcentreLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.CostcentreBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>settlementLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.SettlementBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderstateLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.OrderstateBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.OrderBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reminderLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.ReminderBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>evaluationCreater</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.EvaluationBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>protocolLister</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.ProtocolBackingBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>inputFileController</managed-bean-name>
		<managed-bean-class>at.icnc.om.backingbeans.InputFileController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jspx</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/index.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failure</from-outcome>
			<to-view-id>/login.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>logout</display-name>
		<from-view-id>/index.jspx</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<application>
		<view-handler>com.icesoft.faces.facelets.D2DFaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>de</default-locale>
			<supported-locale>de</supported-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<message-bundle>messages</message-bundle>
	</application>
	<lifecycle>
		<phase-listener>inet.module.mdm.extval.InetStartupListener</phase-listener>
		<!-- <phase-listener>inet.module.mdm.jsf.phaselistener.ValidationMessagePhaseListener</phase-listener>  -->
	</lifecycle>
</faces-config>
