<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	template="/WEB-INF/templates/defaultTemplate.jspx">
			<ice:dataTable value="#{invoiceLister.orderList}" var="item">
				<ice:column>
					<ice:rowSelector></ice:rowSelector>
					<f:facet name="header">ID</f:facet>
					<ice:outputText value="#{item.idInvoice}"/>
				</ice:column>
				<ice:column>
					<f:facet name="header">Faelligkeitsdatum</f:facet>
					<ice:outputText value="#{item.duedate}"/>
				</ice:column>
				<ice:column>
					<f:facet name="header">Rechnungsnummer</f:facet>
					<ice:outputText value="#{item.invoicenumber}"/>
				</ice:column>
				<ice:column>
					<f:facet name="header">Betrag</f:facet>
					<ice:outputText value="#{item.sum}"/>
				</ice:column>
				<ice:column>
					<f:facet name="header">Auftragsdatum</f:facet>
					<ice:outputText value="#{item.tblOrder.orderdate}"/>
				</ice:column>
			</ice:dataTable>
</html>